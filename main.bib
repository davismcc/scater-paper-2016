@ARTICLE{Delmans2016-qc,
  title       = "{Discrete distributional differential expression (D(3)E) - a
                 tool for gene expression analysis of single-cell RNA-seq data}",
  author      = "Delmans, Mihails and Hemberg, Martin",
  affiliation = "Department of Plant Sciences, University of Cambridge, Downing
